---
import Layout from "../layouts/Layout.astro";
import HeaderButton from "../components/HeaderButton.astro";
---

<Layout title="About the project">
  <div class="flex flex-col text-center justify-center">
    <h2 class="mb-4 text-3xl tracking-tight font-extrabold text-white pointer-events-none">
      About the project
    </h2>
    <p class="text-slate-500 sm:text-lx dark:text-slate-400 font-light pointer-events-none">
      The SpaceX launches project is a demo for the Astro demo
    </p>
  </div>
  <section
    class="flex flex-col items-center justify-between w-full text-center md:flex-row my-10"
  >
  <HeaderButton>
    <svg slot="before" width="24" height="24" viewBox="0 0 24 24" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
      <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
      <path d="M5 12l14 0"></path>
      <path d="M5 12l6 6"></path>
      <path d="M5 12l6 -6"></path>
    </svg>
    <a href="/">
      Back to Home
    </a>
  </HeaderButton>
  </section>
</Layout>